services:
  artlist.core:
    build:
      context: D:\Development\Projects\MovieConverter\backend
      dockerfile: Artlist.Core/Dockerfile
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_URLS: https://+:443;http://+:80
    image: artlistcore
    ports:
    - target: 80
    - target: 443
    volumes:
    - C:\Users\alexe\AppData\Roaming\ASP.NET\Https:/root/.aspnet/https:ro
  artlist.server:
    build:
      context: D:\Development\Projects\MovieConverter\backend
      dockerfile: Artlist.Server/Dockerfile
    environment:
      ASPNETCORE_ENVIRONMENT: Development
    image: artlistserver
    ports:
    - target: 80
version: '3.4'